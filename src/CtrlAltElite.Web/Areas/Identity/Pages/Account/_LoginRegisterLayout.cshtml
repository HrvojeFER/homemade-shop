<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Ctrl Alt Elite</title>
    <script src="~/js/styles.js" asp-append-version="true"></script>
</head>
<body>

    <div id="pd"></div>
    <div class="container">
          @RenderBody()
    </div>
    <environment include="Development">
        <script src="~/js/loginReg.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/loginReg.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
